        .design ul li {
            list-style: none;
            color: #fff;
            font-size: 10em;
            font-weight: bold;
            letter-spacing: 10px;
            filter: blur(1px)
        }

        .design ul:hover li{
            animation: animate 2s linear infinite;
            
        }
        @keyframes animate{
            
            0%{
                transform: rotate(0deg) translateY(0px);
                opacity: 1;
                filter: blur(1px);
            }
                
        100%{
                transform: rotate(45deg) translateY(-200px);
                opacity: 1;
                filter: blur(20px);
                
                
            }
            
            
        }
        .design ul li:nth-child(1){
            animation-delay: 0s;

        }
        .design ul li:nth-child(2){
            animation-delay: 0.4s;

        }

        .design ul li:nth-child(3){
            animation-delay: 0.8s;

        }

        .design ul li:nth-child(4){
            animation-delay: 1.2s;

        }
        .design ul li:nth-child(5){
            animation-delay: 1.6s;

        }
        .design ul li{
            color:black;
        }
    </style>
